{
  "name": "pnc-ui-release-tests",
  "version": "1.4.0",
  "description": "Automated release tests for PNC through Web UI",
  "config": {
    "protractorConf": "protractor.config.js"
  },
  "scripts": {
    "test": "npm run lint",
    "integration-test": "protractor $npm_package_config_protractorConf",
    "integration-test:specs": "npm run integration-test -- --specs",
    "integration-test:suite": "npm run integration-test -- --suite",
    "postintegration-test": "webdriver-manager shutdown",
    "lint": "eslint src/**/*.js tests/**/*.js protractor.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/project-ncl/pnc-ui-release-tests.git"
  },
  "author": "Alex Creasy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/project-ncl/pnc-ui-release-tests/issues"
  },
  "homepage": "https://github.com/project-ncl/pnc-ui-release-tests#readme",
  "dependencies": {
    "npm": "^6.3.0",
    "node": "^9.7.1",
    "protractor": "^5.4.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0"
  }
}
